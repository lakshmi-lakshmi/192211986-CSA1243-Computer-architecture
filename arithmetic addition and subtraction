#include <stdio.h>
int main() {
    int num1 = 2;
    int num2 = 3;
    int sum;
    sum = num1 + num2;
    printf("The sum of %d and %d is %d\n", num1, num2, sum);
    return 0;
}
#include <stdio.h>
int main() {
    int num1 = 2;
    int num2 = 3;
    int diff;
    diff = num1 - num2;
    printf("The diff of %d and %d is %d\n", num1, num2, diff);
    return 0;
}

